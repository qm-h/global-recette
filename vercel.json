{
    "version": 2,
    "builds": [
        {
            "src": "./build/server/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["./build/server/**"] }
        }
    ],
    "routes": [
        {
            "src": "/.(*)",
            "dest": "/"
        }
    ]
}
